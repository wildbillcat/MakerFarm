@model IEnumerable<MakerFarm.Models.PrinterType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaterialUseUnit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrinterIcon)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SupportedFileTypes)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TypeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaterialUseUnit)
        </td>
        <td>
            <img src="@Url.Content(model.PrinterIcon)" alt="Image" />
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SupportedFileTypes)
        </td>
        <td>
            @Html.ActionLink("Submit Print", "Create", "Prints", new { id = item.PrinterTypeId })
        </td>
    </tr>
}

</table>
