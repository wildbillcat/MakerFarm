@model IEnumerable<MakerFarm.Models.Print>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FileName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstMaterialUse)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubmissionTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstToolpathTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AuthorizedAttempts)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrinterTypeID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StaffAssitedPrint)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FileName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstMaterialUse)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubmissionTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstToolpathTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AuthorizedAttempts)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrinterTypeID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StaffAssitedPrint)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PrinterTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PrinterTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PrinterTypeID })
        </td>
    </tr>
}

</table>
