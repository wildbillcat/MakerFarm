@model IEnumerable<MakerFarm.Models.Printer>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PrinterName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrinterTypeId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PrinterName)
        </td>
        <td>
            @ViewBag.PrinterNames[item.PrinterTypeId].TypeName
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PrinterId }) |
            @Html.ActionLink("Details", "Details", new { id=item.PrinterId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PrinterId }) | 
            @Html.ActionLink("Update Status", "Create", "PrintStatusLogs", new { id = item.PrinterId }, null)
        </td>
    </tr>
}

</table>
